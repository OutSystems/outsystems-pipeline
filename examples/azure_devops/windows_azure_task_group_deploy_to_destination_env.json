        {
            "environment": {},
            "displayName": "Deploy to Destination Environment",
            "alwaysRun": false,
            "continueOnError": false,
            "condition": "succeeded()",
            "enabled": true,
            "timeoutInMinutes": 0,
            "inputs": {
                "targetType": "inline",
                "filePath": "",
                "arguments": "",
                "script": "$ApplicationScope = Get-Content -Path $(ArtifactsReleaseFolder)\\ApplicationScope.cache\n\npython -m outsystems.pipeline.deploy_latest_tags_to_target_env --artifacts \"$(ArtifactsReleaseFolder)\"  --lt_url $(LifeTimeEnvironmentURL)  --lt_token $(LifeTimeServiceAccountToken)  --lt_api_version $(LifeTimeAPIVersion)  --source_env $(SourceEnvironment)  --destination_env $(DestinationEnvironment)  --app_list $ApplicationScope",
                "errorActionPreference": "stop",
                "failOnStderr": "false",
                "ignoreLASTEXITCODE": "false",
                "pwsh": "false",
                "workingDirectory": ""
            },
            "task": {
                "id": "e213ff0f-5d5c-4791-802d-52ea3e7be1f1",
                "versionSpec": "2.*",
                "definitionType": "task"
            }
        },
        {
            "environment": {},
            "displayName": "Stash generated *.cache files into pipeline logs",
            "alwaysRun": false,
            "continueOnError": true,
            "condition": "always()",
            "enabled": true,
            "timeoutInMinutes": 0,
            "inputs": {
                "targetType": "inline",
                "filePath": "",
                "arguments": "",
                "script": "Write-Host \"Stashing the *.cache generated in the pipeline logs\"\n$cache_files = Get-ChildItem -Path \"$(ArtifactsReleaseFolder)\\*.cache\" -Recurse\nforeach ($cfile in $cache_files) {\n    Write-Host \"Stashing $cfile\"\n    Write-Output \"##vso[task.uploadfile]$cfile\"\n}",
                "errorActionPreference": "stop",
                "failOnStderr": "false",
                "ignoreLASTEXITCODE": "false",
                "pwsh": "false",
                "workingDirectory": ""
            },
            "task": {
                "id": "e213ff0f-5d5c-4791-802d-52ea3e7be1f1",
                "versionSpec": "2.*",
                "definitionType": "task"
            }
        }
    ],
